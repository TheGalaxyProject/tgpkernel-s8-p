/ {
	emul_display: emul_display {
		mode = <0>; /* 0: video mode, 1: DP command mode, 2: MIPI command mode */
		resolution = <800 1280>;
		size = <80 120>;
		timing,refresh = <60>;
		timing,h-porch = <20 20 20>;
		timing,v-porch = <20 20 20>;
