		hdr_num = <0>;		/* max: 4 */
		hdr_type = <0 0 0 0>;	/* 1: DOLBY_VISION, 2: HDR10, 3: HLG */
		hdr_max_luma = <0>;		/* 0 */
		hdr_max_avg_luma = <0>;	/* 0 */
		hdr_min_luma = <0>;		/* 0.0000 */
