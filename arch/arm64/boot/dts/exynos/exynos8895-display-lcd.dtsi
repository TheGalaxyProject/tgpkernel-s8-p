	panel_spi: spi_gpio {
		compatible = "spi-gpio";
		#address-cells = <1>;
		#size-cells = <0>;
		gpio-mosi = <&gpe5 1 0x00>;
		gpio-sck = <&gpe5 3 0x00>;
		gpio-miso = <&gpe5 0 0x00>;
		num-chipselects = <1>;
		cs-gpios = <&gpe5 2 0x00>;

		panel_spi@0 {
			compatible = "panel_spi";
			reg = <0>;
			spi-max-frequency = <1200000>;
			bits-per-word = <8>;
		};
	};
