	ufs@0x11120000 {
		/* ----------------------- */
		/* 1. SYSTEM CONFIGURATION */
		/* ----------------------- */
		compatible ="samsung,exynos-ufs";
		#address-cells = <2>;
		#size-cells = <1>;
		ranges;

		reg =
			<0x0 0x11120000 0x200>,	/* 0: HCI standard */
			<0x0 0x11121100 0x200>,	/* 1: Vendor specificed */
