			pm_qos-max-class = <6>;
			pm_qos-jigbooting = <1469000>;

			/* auto calibration value */
			auto-cal-freq = <1703000>;
			auto-cal-duration = <400>;	/* ms */

			dm-constraints {
				mif-perf {
					const-type = <CONSTRAINT_MIN>;
					dm-type = <DM_MIF>;
						/*  cpu     mif  */
					table = < 2808000 1794000
						  2704000 1794000
						  2652000 1794000
						  2574000 1794000
						  2496000 1794000
						  2314000 1794000
						  2158000 1540000
						  2002000 1540000
						  1937000 1352000
						  1807000 1352000
						  1703000 1014000
						  1469000  845000
