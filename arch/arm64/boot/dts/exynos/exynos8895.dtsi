	cpufreq-ufc {
		/* Ctrl Type */
		#define PM_QOS_MIN_LIMIT		0
		#define PM_QOS_MIN_WO_BOOST_LIMIT	1
		#define PM_QOS_MAX_LIMIT		2
		/* Execution Mode */
		#define	AARCH64_MODE			0
		#define AARCH32_MODE			1

		domain@0 {
			device_type = "cpufreq-userctrl";
			shared-cpus = "0-3";

			user-default-qos = <715000>;
		};

		domain@1 {
			device_type = "cpufreq-userctrl";
			shared-cpus = "4-7";

			user-default-qos = <962000>;

			min-limit {
				ctrl-type = <PM_QOS_MIN_LIMIT>;
				execution-mode = <AARCH64_MODE>;
					/* big	    lit */
				table = < 2314000 1690000
					2158000 715000
					2002000 715000
					1937000 715000
					1807000 715000
					1703000 715000
					1469000 715000
					1261000 715000
					1170000 715000
					1066000 715000
					 962000 715000
					 858000 715000
					 741000 715000>;
			};

			max-limit {
				ctrl-type = <PM_QOS_MAX_LIMIT>;
				execution-mode = <AARCH64_MODE>;
					/* big	    lit */
				table = < 2314000 1690000
					2158000 1690000
					2002000 1690000
					1937000 1690000
					1807000 1690000
					1703000 1690000
					1469000 1690000
					1261000 1690000
					1170000 1690000
					1066000 1690000
					 962000 1690000
					 858000 1690000
					 741000 1690000 >;
			};

		};
	};
