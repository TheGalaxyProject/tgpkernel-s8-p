			<0x0 0x11130000 0x100>;	/* 3: UFS protector */
		interrupts = <0 334 0>;
		pinctrl-names = "default";
		pinctrl-0 = <&ufs_rst_n &ufs_refclk_out>;
		clocks =
			/* aclk clock */
